{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PlayPalDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=PlayPal;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AWS": {
    "Profile": "playpal-profile",
    "ProfilesLocation": "credentials",
    "ServiceURL": "http://127.0.0.1:9000/",
    "SignatureVersion": "v4",
    "AuthenticationRegion": "us-east-1"
  },
  "Objectstore": {
    "DocumentBucket": "profile-pictures",
    "DefaultPresignedUrlValidity": 20
  }
}